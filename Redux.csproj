  <Target Name="PrepareModFolder" AfterTargets="Build" Condition=" '$(Configuration)' == 'Release' ">
    <MakeDir Directories="$(OutputPath)Mods\MSLaFaver-Redux" />
    <Copy SourceFiles="$(TargetPath)" DestinationFolder="$(OutputPath)Mods\MSLaFaver-Redux" SkipUnchangedFiles="true" />
    <Copy SourceFiles="@(ExtraFiles)" DestinationFolder="$(OutputPath)Mods\MSLaFaver-Redux" SkipUnchangedFiles="true" Condition="@(ExtraFiles) != ''" />
    <Copy SourceFiles="@(AssetFiles)" DestinationFiles="@(AssetFiles->'$(OutputPath)Mods\\MSLaFaver-Redux\\Assets\\%(RecursiveDir)%(Filename)%(Extension)')" SkipUnchangedFiles="true" Condition="@(AssetFiles) != ''" />
  </Target>
  <Target Name="CleanBinObj" BeforeTargets="BeforeBuild">
    <RemoveDir Directories="$(ProjectDir)bin\$(Configuration)" Condition="Exists('$(ProjectDir)bin\$(Configuration)')" />
    <RemoveDir Directories="$(ProjectDir)obj\$(Configuration)" Condition="Exists('$(ProjectDir)obj\$(Configuration)')" />
  </Target>
  <Target Name="PackageModZip" AfterTargets="PrepareModFolder" Condition=" '$(Configuration)' == 'Release' ">
    <ReadLinesFromFile File="$(ProjectDir)Properties\AssemblyInfo.cs">
      <Output TaskParameter="Lines" ItemName="AssemblyInfoLines" />
    </ReadLinesFromFile>
    <ItemGroup>
      <MelonLine Include="@(AssemblyInfoLines)" Condition="$([System.String]::Copy('%(AssemblyInfoLines.Identity)').IndexOf('MelonInfo')) &gt; -1" />
    </ItemGroup>
    <PropertyGroup>
      <ModVersion>$([System.Text.RegularExpressions.Regex]::Match('%(MelonLine.Identity)','MelonInfo\([^,]*,\s*"(?:[^"]*)"\s*,\s*"(.*?)"').Groups[1].Value)</ModVersion>
    </PropertyGroup>
    <Message Importance="High" Text="Packaging mod zip with version $(ModVersion)" />
    <Exec Command="powershell -NoProfile -Command Compress-Archive -Path '$(OutputPath)Mods' -DestinationPath '$(OutputPath)MSLaFaver-Redux-$(ModVersion).zip' -Force" />
